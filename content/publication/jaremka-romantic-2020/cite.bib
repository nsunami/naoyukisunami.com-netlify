 journal = {Personal Relationships},
 language = {en},
 note = {_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/pere.12338},
 number = {3},
 pages = {708--727},
 shorttitle = {Romantic relationship distress, gender, socioeconomic status, and inflammation},
 title = {Romantic relationship distress, gender, socioeconomic status, and inflammation: A preregistered report},
